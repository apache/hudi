{
  "type": "record",
  "name": "LogicalTypesSample",
  "namespace": "test",
  "fields": [
    {
      "name": "_row_key",
      "type": "string"
    },
    {
      "name": "partition_path",
      "type": "string"
    },
    {
      "name": "ts",
      "type": ["long", "null"]
    },
    {
      "name": "_hoodie_is_deleted",
      "type": ["boolean", "null"],
      "default" : false
    },
    {
      "name": "double_field",
      "type": "double",
      "default": 0.0
    },
    {
      "name": "float_field",
      "type": "float",
      "default": 0.0
    },
    {
      "name": "int_field",
      "type": "int",
      "default": 0
    },
    {
      "name": "long_field",
      "type": "long",
      "default": 0
    },
    {
      "name": "string_field",
      "type": {
        "type": "string",
        "avro.java.string": "String"
      },
      "default": ""
    },
    {
      "name": "bytes_field",
      "type": "bytes",
      "default": ""
    },
    {
      "name": "decimal_field",
      "type": {
        "type": "fixed",
        "name": "decimal_field_testing",
        "size": 5,
        "logicalType": "decimal",
        "precision": 10,
        "scale": 6
      },
      "default": "\u0000"
    },
    {
      "name": "nested_record",
      "type": [
        "null",
        {
          "type": "record",
          "name": "Nested",
          "namespace": "test",
          "fields": [
            {
              "name": "nested_int",
              "type": "int",
              "default": 0
            },
            {
              "name": "nested_string",
              "type": "string"
            }
          ]
        }
      ],
      "default": null
    },
    {
      "name":"nullable_map_field",
      "type":["null", {
        "type":"map",
        "values": "Nested"
      }],
      "default": null
    },
    {
      "name": "array_field",
      "type": {
        "type": "array",
        "items": "Nested"
      },
      "default": []
    },
    {
      "name": "date_nullable_field",
      "type": [
        "null",
        {
          "type": "int",
          "logicalType": "date"
        }
      ],
      "default": null
    },
    {
      "name": "timestamp_millis_field",
      "type":
        {
          "type": "long",
          "logicalType": "timestamp-millis"
        }
    },
    {
      "name": "timestamp_micros_nullable_field",
      "type": [
        "null",
        {
          "type": "long",
          "logicalType": "timestamp-micros"
        }
      ],
      "default": null
    },
    {
      "name": "timestamp_local_millis_nullable_field",
      "type": [
        "null",
        {
          "type": "long",
          "logicalType": "local-timestamp-millis"
        }
      ],
      "default": null
    },
    {
      "name": "timestamp_local_micros_field",
      "type":
        {
          "type": "long",
          "logicalType": "local-timestamp-micros"
        }
    }
  ]
}